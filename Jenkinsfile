pipeline 
{
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "Hello world!"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Test world!"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploy world!"'
            }
        }
       
    }
}
